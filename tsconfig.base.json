{
  "compilerOptions": {
    "noEmit": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": true,
    "target": "es6",
    "allowJs": true,
    "allowSyntheticDefaultImports": true
  }
}
