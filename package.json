{
  "name": "cs-homepage",
  "private": true,
  "version": "0.1.7-alpha",
  "author": {
    "name": "William Bain"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^4.0.0",
    "expect": "^25.3.0",
    "glob": "^7.1.2",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.1.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.4",
    "request": "^2.88.0",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "selenium-webdriver": "^3.6.0",
    "style-loader": "^1.1.3",
    "stylelint": "^13.3.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "clean": "rm -rf content/home-assets/ content/section-partial/ _site/",
    "fmt": "prettier --write . && stylelint --fix src/scss && eslint --fix .",
    "lint": "eslint . && stylelint src/scss && prettier --check .",
    "pre-jekyll": "yarn run lint-pre-jekyll && yarn run build-pre-jekyll && yarn run verify-bundled",
    "lint-pre-jekyll": "prettier --check . && stylelint src/scss",
    "build-pre-jekyll": "webpack --verbose",
    "verify-bundled": "eslint --no-eslintrc --no-ignore --config .eslintrc.bundled.js content/home-assets",
    "lint-build-infra": "eslint --ignore-pattern src .",
    "jekyll-build": "bundle exec jekyll build --source content",
    "jekyll-serve": "bundle exec jekyll serve --source content -H 0.0.0.0 -P 8080",
    "jekyll-lint": "bundle exec ruby build/jekyll-lint.rb",
    "integration-tests": "mocha --delay integration-tests/main.js"
  },
  "dependencies": {
    "animejs": "^3.0.1",
    "debug": "^3.1.0",
    "normalize.css": "^8.0.0"
  },
  "engines": {
    "yarn": ">= 1.0.0"
  }
}
