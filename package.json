{
  "name": "cs-homepage",
  "private": true,
  "version": "0.1.7-alpha",
  "author": {
    "name": "William Bain"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-transform-modules-commonjs": "^7.23.0",
    "@babel/preset-typescript": "^7.23.2",
    "@types/animejs": "^3.1.10",
    "@types/debug": "^4.1.10",
    "@types/gtag.js": "^0.0.17",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "@types/selenium-webdriver": "^4.1.19",
    "@types/twitter-for-web": "^0.0.4",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "^10.4.16",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "mini-svg-data-uri": "^1.4.3",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "selenium-webdriver": "^4.14.0",
    "style-loader": "^3.3.3",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-order": "^6.0.3",
    "svgo": "^3.0.2",
    "ts-loader": "^9.5.0",
    "typescript": "~5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "clean": "rm -rf content/home-assets/ content/section-partial/ _site/",
    "fmt": "prettier --write . && stylelint --fix src/scss && eslint --fix .  --ext .js,.ts",
    "lint": "eslint . --ext .js,.ts && stylelint src/scss && prettier --check .",
    "pre-jekyll": "pnpm run lint-pre-jekyll && pnpm run build-pre-jekyll && pnpm run verify-bundled",
    "lint-pre-jekyll": "prettier --check . && stylelint src/scss",
    "build-pre-jekyll": "webpack --stats=verbose",
    "verify-bundled": "eslint --no-eslintrc --no-ignore --config .eslintrc.bundled.js content/home-assets",
    "lint-build-infra": "eslint --ignore-pattern src --ext .js,.ts . && tsc -p integration-tests",
    "jekyll-build": "bundle exec jekyll build --source content",
    "jekyll-serve": "bundle exec jekyll serve --source content -H 0.0.0.0 -P 8080",
    "jekyll-lint": "bundle exec ruby build/jekyll-lint.rb",
    "tck": "tsc -p . && tsc -p integration-tests",
    "test": "jest"
  },
  "dependencies": {
    "@sentry/browser": "^7.73.0",
    "animejs": "^3.2.1",
    "debug": "^4.3.4",
    "normalize.css": "^8.0.1"
  },
  "engines": {
    "pnpm": ">= 8.3.1"
  }
}
