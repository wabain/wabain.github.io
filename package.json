  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-typescript": "^7.13.0",
    "@jest/globals": "^27.1.1",
    "@types/animejs": "^3.1.4",
    "@types/debug": "^4.1.7",
    "@types/gtag.js": "^0.0.7",
    "@types/node": "^14.14.22",
    "@types/selenium-webdriver": "^4.0.11",
    "@types/twitter-for-web": "^0.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "autoprefixer": "^10.3.2",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "cssnano": "^5.0.8",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-webpack-plugin": "^3.0.1",
    "jest": "^27.0.6",
    "mini-css-extract-plugin": "^2.2.0",
    "mini-svg-data-uri": "^1.3.3",
    "postcss": "^8.2.9",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "sass": "^1.38.0",
    "sass-loader": "^12.1.0",
    "selenium-webdriver": "^3.6.0",
    "style-loader": "^3.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.20.1",
    "svgo": "^2.5.0",
    "ts-loader": "^9.2.5",
    "typescript": "~4.3.5",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.8.0"
  },
  "scripts": {
    "clean": "rm -rf content/home-assets/ content/section-partial/ _site/",
    "fmt": "prettier --write . && stylelint --fix src/scss && eslint --fix .  --ext .js,.ts",
    "lint": "eslint . --ext .js,.ts && stylelint src/scss && prettier --check .",
    "pre-jekyll": "yarn run lint-pre-jekyll && yarn run build-pre-jekyll && yarn run verify-bundled",
    "lint-pre-jekyll": "prettier --check . && stylelint src/scss",
    "build-pre-jekyll": "webpack --stats=verbose",
    "verify-bundled": "eslint --no-eslintrc --no-ignore --config .eslintrc.bundled.js content/home-assets",
    "lint-build-infra": "eslint --ignore-pattern src --ext .js,.ts . && tsc -p integration-tests",
    "jekyll-build": "bundle exec jekyll build --source content",
    "jekyll-serve": "bundle exec jekyll serve --source content -H 0.0.0.0 -P 8080",
    "jekyll-lint": "bundle exec ruby build/jekyll-lint.rb",
    "tck": "tsc -p . && tsc -p integration-tests",
    "test": "jest"
  },
  "dependencies": {
    "@sentry/browser": "^6.12.0",
    "animejs": "^3.2.1",
    "debug": "^4.3.1",
    "normalize.css": "^8.0.0"
  },
  "engines": {
    "yarn": ">= 1.0.0"
  }
}
