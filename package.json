{
  "name": "cs-homepage",
  "private": true,
  "version": "0.1.7-alpha",
  "author": {
    "name": "William Bain"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "copy-webpack-plugin": "^4.2.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-loader": "^1.9.0",
    "expect": "^21.2.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "glob": "^7.1.2",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.1",
    "postcss-loader": "^3.0.0",
    "request": "^2.83.0",
    "sass-loader": "^6.0.6",
    "selenium-webdriver": "^3.6.0",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "uglifyjs-webpack-plugin": "^1.1.0",
    "url-loader": "^1.0.1",
    "webpack": "^3.8.1"
  },
  "scripts": {
    "clean": "rm -rf content/home-assets/ content/section-partial/ _site/",
    "pre-jekyll": "yarn run build-pre-jekyll && yarn run verify-bundled",
    "build-pre-jekyll": "webpack --verbose",
    "verify-bundled": "eslint --no-eslintrc --ignore-path /dev/null --ignore-pattern vendor --config .eslintrc.bundled.js content/home-assets",
    "lint-build-infra": "eslint --ignore-pattern src .",
    "jekyll-build": "bundle exec jekyll build --source content",
    "jekyll-serve": "bundle exec jekyll serve --source content -H 0.0.0.0 -P 8080",
    "integration-tests": "mocha --delay integration-tests/main.js"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "jquery": "~1.11.0",
    "normalize.css": "^8.0.0"
  },
  "engines": {
    "yarn": ">= 1.0.0"
  }
}
