  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/animejs": "^3.1.4",
    "@types/debug": "^4.1.7",
    "@types/gtag.js": "^0.0.7",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/selenium-webdriver": "^4.0.15",
    "@types/twitter-for-web": "^0.0.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.3.7",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.5.0",
    "cssnano": "^5.0.8",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-webpack-plugin": "^3.0.1",
    "jest": "^27.2.4",
    "mini-css-extract-plugin": "^2.4.3",
    "mini-svg-data-uri": "^1.4.3",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "prettier": "^2.4.1",
    "sass": "^1.43.3",
    "sass-loader": "^12.1.0",
    "selenium-webdriver": "^4.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^2.0.0",
    "stylelint-order": "^5.0.0",
    "svgo": "^2.7.0",
    "ts-loader": "^9.2.5",
    "typescript": "~4.4.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1"
  },
  "scripts": {
    "clean": "rm -rf content/home-assets/ content/section-partial/ _site/",
    "fmt": "prettier --write . && stylelint --fix src/scss && eslint --fix .  --ext .js,.ts",
    "lint": "eslint . --ext .js,.ts && stylelint src/scss && prettier --check .",
    "pre-jekyll": "yarn run lint-pre-jekyll && yarn run build-pre-jekyll && yarn run verify-bundled",
    "lint-pre-jekyll": "prettier --check . && stylelint src/scss",
    "build-pre-jekyll": "webpack --stats=verbose",
    "verify-bundled": "eslint --no-eslintrc --no-ignore --config .eslintrc.bundled.js content/home-assets",
    "lint-build-infra": "eslint --ignore-pattern src --ext .js,.ts . && tsc -p integration-tests",
    "jekyll-build": "bundle exec jekyll build --source content",
    "jekyll-serve": "bundle exec jekyll serve --source content -H 0.0.0.0 -P 8080",
    "jekyll-lint": "bundle exec ruby build/jekyll-lint.rb",
    "tck": "tsc -p . && tsc -p integration-tests",
    "test": "jest"
  },
  "dependencies": {
    "@sentry/browser": "^6.13.3",
    "animejs": "^3.2.1",
    "debug": "^4.3.1",
    "normalize.css": "^8.0.0"
  },
  "engines": {
    "yarn": ">= 1.0.0"
  }
}
