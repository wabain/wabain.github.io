  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-typescript": "^7.12.7",
    "@jest/globals": "^26.6.2",
    "@types/animejs": "^3.1.2",
    "@types/debug": "^4.1.5",
    "@types/gtag.js": "^0.0.4",
    "@types/node": "^14.14.22",
    "@types/selenium-webdriver": "^4.0.11",
    "@types/twitter-for-web": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^4.3.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-webpack-plugin": "^2.4.3",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.4",
    "mini-svg-data-uri": "^1.2.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.0",
    "selenium-webdriver": "^3.6.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "svgo": "^1.3.2",
    "ts-loader": "^8.0.3",
    "typescript": "~4.1.3",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0"
  },
  "scripts": {
    "clean": "rm -rf content/home-assets/ content/section-partial/ _site/",
    "fmt": "prettier --write . && stylelint --fix src/scss && eslint --fix .  --ext .js,.ts",
    "lint": "eslint . --ext .js,.ts && stylelint src/scss && prettier --check .",
    "pre-jekyll": "yarn run lint-pre-jekyll && yarn run build-pre-jekyll && yarn run verify-bundled",
    "lint-pre-jekyll": "prettier --check . && stylelint src/scss",
    "build-pre-jekyll": "webpack --stats=verbose",
    "verify-bundled": "eslint --no-eslintrc --no-ignore --config .eslintrc.bundled.js content/home-assets",
    "lint-build-infra": "eslint --ignore-pattern src --ext .js,.ts . && tsc -p integration-tests",
    "jekyll-build": "bundle exec jekyll build --source content",
    "jekyll-serve": "bundle exec jekyll serve --source content -H 0.0.0.0 -P 8080",
    "jekyll-lint": "bundle exec ruby build/jekyll-lint.rb",
    "tck": "tsc -p . && tsc -p integration-tests",
    "test": "jest"
  },
  "dependencies": {
    "@sentry/browser": "^5.30.0",
    "animejs": "^3.2.1",
    "debug": "^4.3.1",
    "normalize.css": "^8.0.0"
  },
  "engines": {
    "yarn": ">= 1.0.0"
  }
}
