{
  "name": "cs-homepage",
  "private": true,
  "version": "0.1.7-alpha",
  "author": {
    "name": "William Bain"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^4.11.0",
    "eslint-loader": "^2.1.2",
    "expect": "^24.8.0",
    "glob": "^7.1.2",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "request": "^2.83.0",
    "sass-loader": "^7.1.0",
    "selenium-webdriver": "^3.6.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.0",
    "url-loader": "^2.0.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2"
  },
  "scripts": {
    "clean": "rm -rf content/home-assets/ content/section-partial/ _site/",
    "pre-jekyll": "yarn run lint-pre-jekyll && yarn run build-pre-jekyll && yarn run verify-bundled",
    "lint-pre-jekyll": "yarn run stylelint src/scss",
    "build-pre-jekyll": "webpack --verbose",
    "verify-bundled": "eslint --no-eslintrc --ignore-path /dev/null --ignore-pattern vendor --config .eslintrc.bundled.js content/home-assets",
    "lint-build-infra": "eslint --ignore-pattern src .",
    "jekyll-build": "bundle exec jekyll build --source content",
    "jekyll-serve": "bundle exec jekyll serve --source content -H 0.0.0.0 -P 8080",
    "integration-tests": "mocha --delay integration-tests/main.js"
  },
  "dependencies": {
    "animejs": "^3.0.1",
    "debug": "^3.1.0",
    "normalize.css": "^8.0.0"
  },
  "engines": {
    "yarn": ">= 1.0.0"
  }
}
