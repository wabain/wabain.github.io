  "engines": {
    "pnpm": ">=8.3.1 <9"
  },
  "_enginesComment": "TODO: Update pnpm to v9 once dependabot supports the v9 lockfile",
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.7",
    "@babel/plugin-transform-modules-commonjs": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/animejs": "^3.1.12",
    "@types/debug": "^4.1.12",
    "@types/gtag.js": "^0.0.20",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.6",
    "@types/selenium-webdriver": "^4.1.22",
    "@types/twitter-for-web": "^0.0.6",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "autoprefixer": "^10.4.19",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "cssnano": "^7.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-webpack-plugin": "^4.2.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "mini-svg-data-uri": "^1.4.3",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.3.2",
    "sass": "^1.77.6",
    "sass-loader": "^13.3.3",
    "selenium-webdriver": "^4.21.0",
    "style-loader": "^4.0.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-order": "^6.0.4",
    "svgo": "^3.3.2",
    "ts-loader": "^9.5.1",
    "typescript": "~5.4.5",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "clean": "rm -rf content/home-assets/ content/section-partial/ _site/",
    "fmt": "prettier --write . && stylelint --fix src/scss && eslint --fix .  --ext .js,.ts",
    "lint": "eslint . --ext .js,.ts && stylelint src/scss && prettier --check .",
    "pre-jekyll": "pnpm run lint-pre-jekyll && pnpm run build-pre-jekyll && pnpm run verify-bundled",
    "lint-pre-jekyll": "prettier --check . && stylelint src/scss",
    "build-pre-jekyll": "webpack --stats=verbose",
    "verify-bundled": "eslint --no-eslintrc --no-ignore --config .eslintrc.bundled.js content/home-assets",
    "lint-build-infra": "eslint --ignore-pattern src --ext .js,.ts . && tsc -p integration-tests",
    "jekyll-build": "bundle exec jekyll build --source content",
    "jekyll-serve": "bundle exec jekyll serve --source content -H 0.0.0.0 -P 8080",
    "jekyll-lint": "bundle exec ruby build/jekyll-lint.rb",
    "tck": "tsc -p . && tsc -p integration-tests",
    "test": "jest"
  },
  "dependencies": {
    "@sentry/browser": "^8.9.2",
    "animejs": "^3.2.2",
    "debug": "^4.3.4",
    "normalize.css": "^8.0.1"
  }
}
