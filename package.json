  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/animejs": "^3.1.0",
    "@types/debug": "^4.1.5",
    "@types/gtag.js": "^0.0.3",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/selenium-webdriver": "^4.0.9",
    "@types/twitter-for-web": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^4.0.0",
    "jest": "^25.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mini-svg-data-uri": "^1.1.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.4",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "selenium-webdriver": "^3.6.0",
    "style-loader": "^1.1.3",
    "stylelint": "^13.3.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "svgo": "^1.3.2",
    "ts-loader": "^6.2.2",
    "typescript": "~3.7.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "clean": "rm -rf content/home-assets/ content/section-partial/ _site/",
    "fmt": "prettier --write . && stylelint --fix src/scss && eslint --fix .  --ext .js,.ts",
    "lint": "eslint . --ext .js,.ts && stylelint src/scss && prettier --check .",
    "pre-jekyll": "yarn run lint-pre-jekyll && yarn run build-pre-jekyll && yarn run verify-bundled",
    "lint-pre-jekyll": "prettier --check . && stylelint src/scss",
    "build-pre-jekyll": "webpack --verbose",
    "verify-bundled": "eslint --no-eslintrc --no-ignore --config .eslintrc.bundled.js content/home-assets",
    "lint-build-infra": "eslint --ignore-pattern src --ext .js,.ts . && tsc -p integration-tests",
    "jekyll-build": "bundle exec jekyll build --source content",
    "jekyll-serve": "bundle exec jekyll serve --source content -H 0.0.0.0 -P 8080",
    "jekyll-lint": "bundle exec ruby build/jekyll-lint.rb",
    "tck": "tsc -p . -p integration-tests",
    "test": "jest"
  },
  "dependencies": {
    "@sentry/browser": "^5.15.4",
    "animejs": "^3.2.0",
    "debug": "^4.1.1",
    "normalize.css": "^8.0.0"
  },
  "engines": {
    "yarn": ">= 1.0.0"
  }
}
